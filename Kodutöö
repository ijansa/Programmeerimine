print "Tere"
Eesnimi = raw_input('Mis su eesnimi on?:')
Perekonnanimi = raw_input('Mis su perekonnanimi on?:')

Eesnimi = Eesnimi.upper()
Perekonnanimi = Perekonnanimi.upper()

print Eesnimi.upper();
print Perekonnanimi.lower();
print Eesnimi.capitalized();
print Perekonnanimi.join();
print Perekonnanimi.center();


if Eesnimi == "Jaan-Edgar":
	print "Sinu nimi on " + Eesnimi + " " + Perekonnanimi
else:

	print "ei tunne sind"
